@model IEnumerable<ShoppicaMVC.ViewModels.CategoryVM>

@for (int i = 0; i < 6; i++)
{

    <div class="grid_2">
        <h2>@Model.ToList()[i].CategoryName</h2>
        <ul class="s_list_1">
            @foreach (var altItem in Model.ToList()[i].InverseTop)

            {
                <li><a href="">@altItem.CategoryName</a></li>
            }

        </ul>
    </div>
}